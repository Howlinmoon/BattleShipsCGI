CREATE TABLE master_ship_chart ( ship_id mediumint not null primary key, ship_name varchar(50) not null, ship_owner varchar(50) not null, ship_country varchar(50) not null, ship_heading varchar(6) not null, ship_x varchar(12) not null, ship_y varchar(12) not null, ship_speed varchar(5) not null, ship_max_speed mediumint(9) not null, ship_sighting_range varchar(12) not null, ship_sight_factor varchar(5) not null,  hull_class varchar(20) not null, depth mediumint(9) not null, task_force mediumint(9) );
